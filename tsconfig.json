{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": true,
    "noImplicitAny": false,
    "outDir": "dist/dev/app"
  },
  "exclude": [
    "node_modules",
    "build",
    "config",
    "dist",
    "typings",
    "app/assets",
    "app/styles"
  ]
}
