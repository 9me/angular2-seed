{
  "name": "MeanieAngular2Seed",
  "description": "Seed project for Angular 2 applications",
  "version": "1.0.0",
  "homepage": "https://github.com/meanie/angular2-seed",
  "author": {
    "name": "Adam Buczynski",
    "email": "me@adambuczynski.com",
    "url": "http://adambuczynski.com/"
  },
  "contributors": [],
  "license": "SEE LICENSE IN LICENSE.md",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/meanie/angular2-seed"
  },
  "bugs": {
    "url": "https://github.com/meanie/angular2-seed/issues"
  },
  "keywords": [],
  "engines": {
    "node": ">=6.2.0",
    "npm": ">=3.8.9"
  },
  "scripts": {
    "postinstall": "typings install",
    "reinstall": "rimraf node_modules && npm cache clean && npm install",
    "prebuild": "tsc",
    "build": "gulp build --env dev",
    "build:prod": "gulp build --env prod",
    "serve": "node build/serve.js --env dev",
    "serve:prod": "node build/serve.js --env prod",
    "prestart": "npm run build -s",
    "start": "parallelshell \"npm run serve -s\" \"npm run watch -s\"",
    "dev": "npm start",
    "lint": "gulp lint",
    "pretest": "npm run lint -s",
    "test": "gulp test",
    "watch": "gulp watch",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@angular/common":  "2.0.0-rc.1",
    "@angular/compiler":  "2.0.0-rc.1",
    "@angular/core":  "2.0.0-rc.1",
    "@angular/http":  "2.0.0-rc.1",
    "@angular/platform-browser":  "2.0.0-rc.1",
    "@angular/platform-browser-dynamic":  "2.0.0-rc.1",
    "@angular/router":  "2.0.0-rc.1",
    "core-js": "^2.4.0",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "^0.19.27",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "del": "^2.2.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-file": "^0.2.0",
    "rimraf": "^2.5.0",
    "typescript": "^1.8.10",
    "typings":"^1.0.4",
    "yamljs": "^0.2.4",
    "yargs": "^3.31.0"
  }
}
